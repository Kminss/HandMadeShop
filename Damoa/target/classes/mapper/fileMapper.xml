<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.javajaba.damoa.upload">

	<insert id="addFile">
	<selectKey keyProperty="fileno" resultType="integer"
			order="BEFORE">
			select hand_seq.currentval() from dual
		</selectKey>
	insert into handfile (
	filename, fileno
	) values (
	#{filename},#{fileno} 
	)
	</insert>

</mapper>